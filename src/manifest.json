{
    "name": "Download links to images",
    "description": "Downloads a .txt file with all links to images on the page.",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16":"icons/16.png",
        "32":"icons/32.png",
        "48":"icons/48.png",
        "128":"icons/128.png"
    },
    "action": {
        "default_icon": {
            "16":"icons/16.png",
            "32":"icons/32.png",
            "48":"icons/48.png",
            "128":"icons/128.png"
        },
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab", "scripting"]
}